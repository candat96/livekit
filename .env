LIVEKIT_API_KEY=devkey
LIVEKIT_API_SECRET=secret

# Alternative production-like key (32+ characters recommended for secrets)
# LIVEKIT_API_KEY=APIKeyForLocalDev
# LIVEKIT_API_SECRET=ThisIsAVeryLongSecretKeyForLocalDevelopmentOnly123

# Domain settings
LIVEKIT_DOMAIN=localhost
LIVEKIT_WS_URL=ws://localhost:7880
LIVEKIT_HTTP_URL=http://localhost:7880

# Port configuration
LIVEKIT_HTTP_PORT=7880
LIVEKIT_RTC_TCP_PORT=7881
LIVEKIT_TURN_PORT=7882
LIVEKIT_UDP_PORT_START=50000
LIVEKIT_UDP_PORT_END=60000

# Redis configuration
REDIS_HOST=redis
REDIS_PORT=6379

# For applications connecting to LiveKit
# These can be used by your client applications
NEXT_PUBLIC_LIVEKIT_URL=ws://localhost:7880
LIVEKIT_URL=ws://localhost:7880